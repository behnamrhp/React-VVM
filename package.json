{
  "name": "reactvvm",
  "version": "1.1.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/reactvvm.umd.js",
  "module": "./dist/reactvvm.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/reactvvm.es.js",
      "require": "./dist/reactvvm.umd.js",
      "types": "./dist/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/behnamrhp/React-VVM"
  },
  "author": {
    "name": "Behnam RHP",
    "email": "behnamrahimpour74@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "vvm",
    "mvvm",
    "view",
    "viewmodel",
    "viewmodel-pattern",
    "model-view-viewmodel",
    "react-vvm",
    "react-vvm-di",
    "react-vvm-di-container"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "eslint": "^9.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "prettier": "^3.3.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3",
    "@types/node": "^22.13.10"
  }
}
